{{#each stars}}
<div style="position:relative; top: {{Y}}px; left: {{X}}px;">
    <table>
        <tr>
            <td rowspan="2" style="width: 50px; height: 50px;" class="star star-icon type-{{CssClass}}"></td>
            <td class="star star-name">{{Name}}</td>
        </tr>
        <tr>
            <td>{{Name}}</td>
        </tr>
    </table>
</div>
{{/each}}